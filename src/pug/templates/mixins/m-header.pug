
-
	navMenu = {
		gl_class: 'nav',
		items: [
			{title: 'About Us',},
			{title: 'Knowledge Hub',},
			{title: 'Innovators Directory',},
			{title: 'Application Portal',},
			{title: 'News & Updates'},
		],
	}

	social = {
		items: [
			{
			mod_link: 'instagram',
			label: 'instagram',
			name_icon: 'instagram', 
			mod_icon: 'instagram',
			},

			{
				mod_link: 'facebook',
				label: 'facebook',
				name_icon: 'facebook', 
				mod_icon: 'facebook',
				},
			{
				mod_link: 'twitter',
				label: 'twitter',
				name_icon: 'twitter', 
				mod_icon: 'twitter',
				},
		]
	}

mixin header
	header.header
		.container 
			nav.nav 
				a.logo(href="#")
					img.logo__img(src="images/logo.png", alt="Re-imagining TB Care") 
				button.burger_btn.js-burger-btn(type='button', aria-label='open menu') menu 
				.nav__menu 
					.nav__menu_wrapper 
							+menu(navMenu)
							+social(social)